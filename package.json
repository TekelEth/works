    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.1",
    "react-spinners": "^0.14.1",
    "react-toastify": "^10.0.5",
    "tailwind-merge": "^2.4.0",
